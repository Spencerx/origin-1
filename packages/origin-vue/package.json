{
  "name": "@app-research/origin-vue",
  "version": "0.3.0-beta.8",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "dev": "npm run build:esm -- -w --incremental & npm run build:cjs -- -w --incremental",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./vite": {
      "require": "./dist/cjs/node/vite/index.js",
      "import": "./dist/esm/node/vite/index.js"
    },
    "./config/tsconfig.base": "./config/tsconfig.base.json",
    "./config/prettier": "./config/prettier.json"
  },
  "typesVersions": {
    "*": {
      "vite": [
        "./dist/esm/node/vite/index.d.ts"
      ]
    }
  },
  "bin": {
    "origin": "./bin/cli.js"
  },
  "dependencies": {
    "@nuxt/devalue": "^2.0.0",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/server-renderer": "^3.2.19",
    "@vueuse/core": "^6.5.3",
    "@vueuse/head": "^0.5.1",
    "cac": "^6.7.3",
    "critters": "^0.0.10",
    "ipfs-base": "^1.1.0",
    "pinia": "^2.0.0-rc.10",
    "unplugin-vue-components": "^0.15.6",
    "vite": "^2.6.2",
    "vite-plugin-pages": "^0.18.1",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-ssg": "^0.15.4",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11",
    "vue-tsc": "^0.3.0"
  },
  "files": [
    "dist",
    "config",
    "bin"
  ],
  "peerDependencies": {
    "@app-research/eslint-config": "^0.1.3",
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}