{
  "name": "@app-research/origin-vue",
  "version": "0.3.0-beta.15",
  "main": "dist/client/index.js",
  "module": "dist/client/index.js",
  "scripts": {
    "dev": "npm run build:esm -- -w --incremental & npm run build:cjs -- -w --incremental",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p src/client && tsc -p src/node",
    "build:cjs": "tsc -p src/node/tsconfig.cjs.json",
    "prepublishOnly": "npm run build",
    "release": "standard-version -t origin-vue@ --path . -p beta",
    "publish": "git push --follow-tags origin main && pnpm publish --tag beta"
  },
  "exports": {
    ".": {
      "import": "./dist/client/index.js"
    },
    "./vite": {
      "require": "./dist/node/cjs/vite-config/index.js",
      "import": "./dist/node/esm/vite-config/index.js"
    },
    "./config/tsconfig.base": "./config/tsconfig.base.json",
    "./config/prettier": "./config/prettier.json",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "vite": [
        "./dist/node/esm/vite-config/index.d.ts"
      ]
    }
  },
  "bin": {
    "origin": "./bin/cli.js"
  },
  "dependencies": {
    "@nuxt/devalue": "^2.0.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vueuse/core": "^6.7.3",
    "@vueuse/head": "^0.6.0",
    "cac": "^6.7.11",
    "critters": "^0.0.14",
    "ipfs-base": "^1.1.0",
    "pinia": "^2.0.0-rc.15",
    "unplugin-vue-components": "^0.16.0",
    "vite": "^2.6.12",
    "vite-plugin-pages": "^0.18.1",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-ssg": "^0.15.5",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.28.9"
  },
  "files": [
    "dist",
    "config",
    "bin",
    "client.d.ts"
  ],
  "peerDependencies": {
    "@app-research/eslint-config": "^0.1.5"
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "rimraf": "^3.0.2"
  }
}