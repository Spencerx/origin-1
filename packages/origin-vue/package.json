{
  "name": "@app-research/origin-vue",
  "version": "0.3.0-beta.3",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "dev": "npm run build:esm -- -w --incremental & npm run build:cjs -- -w --incremental",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "npm": ">=7",
    "node": ">=12"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./vite": {
      "require": "./dist/cjs/vite/index.js",
      "import": "./dist/esm/vite/index.js"
    },
    "./config/tsconfig.base": "./config/tsconfig.base.json",
    "./config/prettier": "./config/prettier.json"
  },
  "typesVersions": {
    "*": {
      "vite": [
        "./dist/esm/vite/index.d.ts"
      ]
    }
  },
  "bin": {
    "origin": "./bin/cli.js"
  },
  "dependencies": {
    "@nuxt/devalue": "^2.0.0",
    "@types/node": "^16.9.2",
    "@vitejs/plugin-vue": "^1.6.2",
    "@vue/compiler-sfc": "^3.2.12",
    "@vue/server-renderer": "^3.2.12",
    "@vueuse/core": "^6.4.0",
    "@vueuse/head": "^0.5.1",
    "autoprefixer": "^10.3.4",
    "cac": "^6.7.3",
    "critters": "^0.0.10",
    "ipfs-base": "^1.1.0",
    "pinia": "^2.0.0-rc.9",
    "postcss-nested": "^5.0.6",
    "unplugin-vue-components": "^0.15.2",
    "vite-plugin-pages": "^0.18.1",
    "vite-plugin-vue-layouts": "^0.4.1",
    "vite-ssg": "^0.15.1",
    "vue-tsc": "^0.3.0"
  },
  "files": [
    "dist",
    "config",
    "bin"
  ],
  "peerDependencies": {
    "@app-research/eslint-config": "^0.1.2",
    "typescript": "^4.4.3",
    "vue": "^3.2.9",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}
